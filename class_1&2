#assingment operators  with in assign for operators

num1:int=5
num2:int=5

sum:int=num1-num2
print(sum)

sum:int=num1+num2
print(sum)

sub:int=num1-num2
print(sub)

mul:int=num1%num2
print(mul)

div:int=num1/num2
print(div)

